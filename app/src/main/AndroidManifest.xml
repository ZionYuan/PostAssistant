<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yz.postassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="学子驿站助手"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainInterfaceActivity" />
        <activity android:name=".EditMessageActivity" />
        <activity
            android:name=".ExpressTrackingActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".AddressListActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".MessageModelActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity
            android:name=".SignHistoryActivity"
            android:theme="@style/Theme.AppCompat.Light" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".Register2Activity"></activity>
    </application>

</manifest>